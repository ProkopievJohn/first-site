.contact
	width 100%
	height 100vh
	overflow hidden
.contact-info
	position absolute
	top 50%
	left 50%
	transform translate(-50%,-50%)
	.contact-log
		margin auto
		width 40%
		img
			width 100%
	.social-list
		margin 10% auto
		text-align center
		li
			width 5%
			display inline-block
			list-style none
			margin 20px
		svg
			width 100%
			height auto
.contact-img
	position absolute
	top 50%
	left 50%
	transform translate(-50%,-50%)
	width 100%
	height 100vh
	.img
		opacity: 0.1
		background-position: center center
		background-size cover
		width 150%
		height 150vh