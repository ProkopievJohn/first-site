.contact
	position absolute
	top 0
	left 0
	width 100%
	height 100vh
	overflow hidden
.contact-info
	position absolute
	top 50%
	left 50%
	transform translate(-50%,-50%)
	.contact-log
		margin auto
		width 40%
		img
			width 100%
	.social-list
		margin 10% auto
		text-align center
		li
			width 60px
			display inline-block
			list-style none
			margin 10px
		svg
			width 100%
			height auto